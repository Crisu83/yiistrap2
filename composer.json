{
    "name": "crisu83/yiistrap2",
    "description": "Twitter Bootstrap for the Yii PHP framework.",
    "keywords": ["bootstrap", "yii2"],
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Christoffer Niska",
            "email": "christoffer.niska@gmail.com",
            "homepage": "http://www.cniska.net/"
        }
    ],
    "require-dev": {
        "php": ">=5.3.7",
        "codeception/codeception": "@stable",
        "yiisoft/yii2": "dev-master",
        "yiisoft/yii2-composer": "dev-master"
    },
    "minimum-stability": "dev",
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\InstallHandler::setPermissions"
        ]
    },
    "extra": {
        "yii-install-writable": [
            "tests/_log"
        ]
    },
    "autoload": {
        "psr-0": {
            "yiistrap\\": "./"
        }
    }
}
