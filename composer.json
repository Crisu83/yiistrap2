{
    "name": "crisu83/yiistrap2",
    "description": "Twitter Bootstrap for Yii2.",
    "keywords": ["yiistrap", "bootstrap", "yii2"],
    "type": "yii-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Christoffer Niska",
            "email": "christoffer.niska@gmail.com",
            "homepage": "http://crisu83.me/"
        }
    ],
    "require-dev": {
        "codeception/codeception": "@stable",
        "twbs/bootstrap": "3.1.*",
        "yiisoft/yii2": "dev-master",
        "yiisoft/yii2-codeception": "dev-master",
        "yiisoft/yii2-composer": "dev-master"
    },
    "minimum-stability": "dev",
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\InstallHandler::setPermissions"
        ]
    },
    "extra": {
        "yii-install-writable": [
            "tests/_log"
        ]
    },
    "autoload": {
        "psr-4": { "yiistrap\\": "" }
    }
}
